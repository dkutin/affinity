  "name": "affinity",
  "description": "Authentication server for Spotify affinity.",
  "version": "1.0.0",
  "main": "server.js",
  "repository": "git@github.com:dkutin/affinity.git",
  "private": true,
  "license": "MIT",
  "dependencies": {
    "concurrently": "^5.2.0",
    "cookie-parser": "^1.4.5",
    "core-util-is": "^1.0.2",
